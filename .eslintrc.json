{
  "parser":"babel-eslint",
  "env":{
    "browser":true,
    "es6":true,
    "node":true
  },
  "extends":[
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "installedESLint":true,
  "parserOptions":{
    "ecmaFeatures":{
      "experimentalObjectRestSpread":true,
      "jsx":true
    },
    "sourceType":"module"
  },
  "plugins":[
    "react"
  ],
  "rules":{
    "indent":[
      "error",
      2,
      {
        "SwitchCase":1
      }
    ],
    "linebreak-style":[
      "error",
      "unix"
    ],
    "camelcase":[
      1
    ],
    "no-unused-vars":[
      "error",
      {
        "vars":"all",
        "args":"after-used"
      }
    ],
    "react/prop-types":[
      0
    ],
    "react/react-in-jsx-scope":"off",
    "no-unexpected-multiline":[
      1
    ],
    "react/display-name":[
      0
    ],
    "no-unused-vars":[
      "error",
      {
        "argsIgnorePattern":"^_"
      }
    ]
  }
}
